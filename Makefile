install :
	sudo apt-get install npm
	sudo apt-get install vim
	sudo apt-get install postgresql-9.5
	sudo npm install node.js -g
	sudo npm install express --save 
	sudo npm install forever -g 
	sudo npm install pug --save  
	sudo npm install http --save 
	sudo npm install helmet --save
	sudo npm install pg --save
	sudo npm install express-session --save
	sudo npm install express-domain-middleware --save
	sudo npm install cookie-parser --save
	sudo npm install body-parser --save
	sudo npm install connect-mongo --save
	sudo ln -s /usr/bin/nodejs /usr/local/bin/node
